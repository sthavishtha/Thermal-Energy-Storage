plot "error_ovs_fluid_part3_pe0_001_n5.txt" using (log($1)):(log($2)) with linespoints lw 2 lc "green" title "L1 - error" ,\
	"error_ovs_fluid_part3_pe0_001_n5.txt" using (log($1)):(log($3)) with linespoints lw 2 lc "black" title "L2 - error" ,\
	"error_ovs_fluid_part3_pe0_001_n5.txt" using (log($1)):(log($4)) with linespoints lw 2 lc "red" title "L-inf - error" 

set output

