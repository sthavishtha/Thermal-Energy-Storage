set term postscript eps color enhanced "Times-Roman" 25

set output "error_ovs_fluid_part3_pe0_001_n5_1.eps" 
#set xrange[-:-5.8] 
set yrange [-9:-1]
set mxtics 10
set mytics 10

set xlabel "logh"
